    rdfs:label     "Slovak" ;
    warehouse:code "sk" .

<http://warehouse.org/language/2>
    a              warehouse:Language ;
    rdfs:label     "English" ;
    warehouse:code "en" .

<http://warehouse.org/language/3>
    a              warehouse:Language ;
    rdfs:label     "German" ;
